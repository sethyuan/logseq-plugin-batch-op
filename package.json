{
  "name": "logseq-plugin-batch-op",
  "version": "0.3.4",
  "main": "dist/index.html",
  "logseq": {
    "id": "_sethyuan-logseq-batch-op",
    "icon": "./icon.png"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "mkdir -p dist && cp index-dev.html dist/index.html && preact watch --no-prerender --no-sw",
    "build": "preact build --no-prerender --no-sw --no-inline-css --no-esm --dest dist && rm dist/push-manifest.json"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@logseq/libs": "^0.0.9",
    "antd": "^4.21.0",
    "immer": "^9.0.15",
    "logseq-l10n": "^0.2.0",
    "preact": "^10.7.3",
    "preact-render-to-string": "^5.2.0",
    "rambdax": "^8.0.1",
    "reactutils": "^5.11.0"
  },
  "devDependencies": {
    "preact-cli": "^3.3.5",
    "preact-cli-svg-loader": "^1.1.0"
  }
}
